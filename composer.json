{
    "name": "yourname/ticketmaster-php",
    "description": "Ticket Management App with PHP and Twig",
    "type": "project",
    "require": {
        "php": "^8.4.14",
        "twig/twig": "^3.0",
        "symfony/routing": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/var-dumper": "^6.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }, 
    "scripts": {
        "post-autoload-dump": [
            "rm -rf public/assets/cache/*"
        ]
    }
}